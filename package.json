{
  "name": "flem-file-upload-api",
  "description": "API que consome requisições das aplicações FLEM para manipulação e armazenamento de arquivos.",
  "version": "v1.0.4-230624",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4020",
    "build": "next build",
    "docs": "jsdoc --readme README.md -c jsdoc-config.json",
    "start": "next start",
    "lint": "next lint",
    "db-push:dev": "dotenv -e .env.development -- yarn prisma db push",
    "studio:dev": "dotenv -e .env.development -- yarn prisma studio",
    "docker:build": "docker build . -t ghcr.io/frtechdev/flem-mail-api:latest",
    "docker:push": "docker push ghcr.io/frtechdev/flem-mail-api:latest",
    "caniuse": "npm update --save caniuse-lite browserslist",
    "clean": "yarn autoclean --force",
    "git:clear": "bash cleargitcache.sh",
    "prisma:dbml": "yarn add prisma-dbml-generator --dev && yarn prisma generate && yarn remove prisma-dbml-generator --dev"
  },
  "dependencies": {
    "@prisma/client": "^4.16.1",
    "clean-jsdoc-theme": "^4.2.9",
    "jsdoc": "^4.0.2",
    "luxon": "^3.0.4",
    "multer": "^1.4.5-lts.1",
    "next": "12.3.1",
    "next-connect": "^0.13.0",
    "nextjs-cors": "^2.1.1",
    "prisma": "^4.16.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "dotenv-cli": "^6.0.0",
    "eslint": "8.23.0",
    "eslint-config-next": "12.2.5"
  }
}
